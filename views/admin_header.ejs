<%
let items = {
  info: '統計訊息',
  config: '系統設定',
  privilege: '權限管理',
  rejudge: '一鍵重測',
  links: '友情連結管理',
  rating: '積分管理',
  raw: '配置文件',
  other: '其他操作'
};
%>
<% this.title = items[this.adminPage] + ' - 後台管理'; %>
<% include header %>
<h1 style="text-align: center; margin-bottom: 27px; ">後台管理</h1>
<div class="ui grid">
  <div class="three wide column">
    <div class="ui vertical fluid tabular menu" style="height: 100%; ">
