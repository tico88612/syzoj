      <a class="item<% if (this.adminPage === x) { %> active<% } %>"<% if (this.adminPage !== x) { %> href="<%= syzoj.utils.makeUrl(['admin', x]) %>"<% } %>>
        <%= items[x] %>
      </a>
    <% } %>
    </div>
  </div>
  <div class="thirteen wide stretched column">
